<div class="sessions-container">
  <h2 style='text-align:center; margin-bottom:20px'>Crea un Producto</h2>
    <%= simple_form_for(@product) do |f| %>
      <%= f.input :title%>
      <%= f.input :price %>
      <%= f.input :description %>
      <%= f.input :age %>
      <%= f.input :dimensions %>
      <div style='text-align:center; margin-top:30px'>
        <%= f.submit class:'btn btn-flat' %>
      </div>
    <% end %>
</div>